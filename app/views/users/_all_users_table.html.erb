<h4>All users</h4>
<table class="table table-bordered">
  <tr>
  	<th>Email</th>
    <th>Name</th>
    <th>Phone Number</th>
  	<th>Registeration</th>
    <th>Last Update</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.name %></td>
      <td><%= user.phone %></td>
      <td><%= user.created_at %></td>
      <td><%= user.updated_at %></td>
    </tr>
  <% end %>
</table>